<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<Group xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.jazhead.timetracking.controller.scene.MainController">
    <children>
        <StackPane prefHeight="150.0" prefWidth="200.0" style="-fx-background-color: #556699;">
            <children>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
                    <children>
                        <ComboBox fx:id="comboBox" />
                        <Label fx:id="screenName" text="SCREEN1" />
                        <HBox prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                                <TextField fx:id="textField" />
                            </children>
                        </HBox>
                        <Button mnemonicParsing="false" text="Screen2" />
                        <Button mnemonicParsing="false" text="Screen3" />
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </VBox>
            </children>
        </StackPane>
    </children>
</Group>
